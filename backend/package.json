{
  "name": "falcon-pass-backend",
  "version": "0.1.0",
  "description": "Secure zero-knowledge password manager backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make"
  },
  "keywords": [
    "password-manager",
    "security",
    "zero-knowledge",
    "encryption",
    "fastify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^9.1.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/jwt": "^7.2.3",
    "@fastify/swagger": "^8.12.0",
    "@simplewebauthn/server": "^8.3.4",
    "@types/jsonwebtoken": "^9.0.10",
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "mysql2": "^3.6.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "sodium-native": "^4.0.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^0.34.6"
  }
}
